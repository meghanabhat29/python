interval=[0,1,2,4]
category=['<1','1 to 2','>2']
iris1['Petal_cat']=pd.cut(iris1[' Petal_Width'],interval,labels=category)
ax=sns.countplot(hue="Class",palette="Set1",data=iris1,x="Petal_cat")
ax.set(title="Petal width",xlabel="cat of petal",ylabel="no. of flowers")
plt.show()

